#include <bits/stdc++.h>
using namespace std;

int main()
{
	int a,b,c;
	cin>>a;
	cin>>b;
	cin>>c;
	if (a<b)
	{
	    if(a<c)
	    {
	        cout<<"a is smallest";
	    }
	    else
	    {
	        cout<<"c is smallest";
	    }
	}
	else
	{
	    if (b<c)
	    {
	        cout<<"b is smallest";
	    }
	    else
	    {
	        cout<<"c is smallest";
	    }
	}
}
